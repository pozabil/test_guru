<tr>
  <% return if test.questions.size == 0 %>
  <td><em><%= test.title %></em></td>
  <td><%= test_level(test) %></td>
  <td><%= test.category.title %></td>
  <td><%= test.questions.size %></td>
  <td><%= test.creator.full_name %></td>
  <td><%= link_to t('open'), test_path(test), class: "link-secondary" %></td>
  <td></td>
  <td><%= start_or_continue_test_button(test) %></td>
</tr>
